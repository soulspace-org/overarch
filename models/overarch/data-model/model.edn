;;;;
;;;; Logical Data Model of Overarch
;;;;
#{;;;
  ;;; Abstract types
  ;;;
  {:el :class
   :id :overarch.data-model/element
   :abstract true
   :name "element"
   :desc "An element of data."
   :tags #{"type-hierarchy"}
   :ct #{{:el :field
          :name "el"
          :type "keyword"
          :desc "The type of the element."}}}

  {:el :class
   :id :overarch.data-model/identifiable-element
   :abstract true
   :name "identifiable-element"
   :desc "An element with an identifier.
          Can be referenced from views and other parts of the model."
   :tags #{"type-hierarchy"}
   :ct #{{:el :field
          :name "id"
          :type "keyword"
          :desc "The identifier of the element."}}}

  {:el :class
   :id :overarch.data-model/named-element
   :abstract true
   :name "named-element"
   :desc "An element with a name and a description."
   :tags #{"type-hierarchy"}
   :ct #{{:el :field
          :name "name"
          :type "string"
          :desc "Name of the element."}
         {:el :field
          :name "desc"
          :type "string"
          :optional true
          :desc "Description of the element."}}}

  {:el :class
   :id :overarch.data-model/identifiable-named-element
   :abstract true
   :name "identifiable-named-element"
   :desc "An element that is identifiable and named."
   :tags #{"type-hierarchy"}}

  {:el :class
   :id :overarch.data-model/compound-node
   :abstract :true
   :name "compound-node"
   :desc "An element which is composed of other elements."
   :tags #{"type-hierarchy"}
   :ct [{:el :field
         :name "ct"
         :type "collection"
         :desc "Collection of elements, e.g. a set or a vector, if the elements need to be in order."}]}

  {:el :class
   :id :overarch.data-model/technical-element
   :abstract true
   :name "technical-element"
   :desc "An element which is implemented in the given technologies."
   :tags #{"type-hierarchy"}
   :ct #{{:el :field
          :name "tech"
          :type "string"
          :optional true
          :desc "Technology the element is implemented in."}}}

  {:el :class
   :id :overarch.data-model/relation
   :abstract true
   :name "relation"
   :desc "An element which describes the relation of elements."
   :tags #{"type-hierarchy"}
   :ct #{{:el :field
          :name "from"
          :type "keyword"
          :desc "The identifier of the element referring to another element."}
         {:el :field
          :name "to"
          :type "keyword"
          :desc "The identifier of the element being referred by another element."}}}

   ;; Abstract Type Relations
  {:el :inheritance
   :id :overarch.data-model/identifiable-element-is-a-element
   :to :overarch.data-model/element
   :from :overarch.data-model/identifiable-element
   :tags #{"type-hierarchy"}}

  {:el :inheritance
   :id :overarch.data-model/compound-node-is-a-identifiable-element
   :to :overarch.data-model/identifiable-element
   :from :overarch.data-model/compound-node
   :tags #{"type-hierarchy"}}

  {:el :inheritance
   :id :overarch.data-model/named-element-is-a-element
   :to :overarch.data-model/element
   :from :overarch.data-model/named-element
   :tags #{"type-hierarchy"}}

  {:el :inheritance
   :id :overarch.data-model/identifiable-named-element-is-a-identifiable-element
   :to :overarch.data-model/identifiable-element
   :from :overarch.data-model/identifiable-named-element
   :tags #{"type-hierarchy"}}

  {:el :inheritance
   :id :overarch.data-model/identifiable-named-element-is-a-named-element
   :to :overarch.data-model/named-element
   :from :overarch.data-model/identifiable-named-element
   :tags #{"type-hierarchy"}}

  {:el :inheritance
   :id :overarch.data-model/relation-is-a-identifiable-element
   :to :overarch.data-model/identifiable-element
   :from :overarch.data-model/relation
   :tags #{"type-hierarchy"}}

  {:el :class
   :id :overarch.data-model/ref
   :name "ref"
   :desc "A reference to an element."
   :ct #{{:el :field
          :name "ref"
          :type "keyword"
          :desc "The identifier the reference is referring to."}}}

  ;;
  ;; Model hierarchies
  ;;
  {:el :class
   :id :overarch.data-model/model-element
   :abstract true
   :name "model-element"
   :desc "An element of the overarch model."
   :tags #{"element-hierarchy"}}

  {:el :class
   :id :overarch.data-model/model-node
   :abstract true
   :name "model-node"
   :desc "A node of the overarch model."
   :tags #{"node-hierarchy"}}

  {:el :class
   :id :overarch.data-model/model-relation
   :abstract true
   :name "model-relation"
   :desc "A relation of the overarch model."
   :tags #{"relation-hierarchy"}}

  {:el :inheritance
   :id :overarch.data-model/model-node-is-a-model-element
   :to :overarch.data-model/model-element
   :from :overarch.data-model/model-node
   :tags #{"node-hierarchy"}}

  {:el :inheritance
   :id :overarch.data-model/model-relation-is-a-model-element
   :to :overarch.data-model/model-element
   :from :overarch.data-model/model-relation
   :tags #{"relation-hierarchy"}}

  ;;
  ;; Generic Relation 
  ;;
  {:el :class
   :id :overarch.data-model/rel
   :name "rel"
   :desc "A generic relation between two elements of the model.
          A rel can be used in the architecture model and the concept model."
   :tags #{"architecture-model" "concept-model"}}

  ;;
  ;; Boundaries
  ;;
  {:el :class
   :id :overarch.data-model/boundary-node
   :abstract true
   :name "boundary-node"
   :desc "A grouping of elements belonging together in a context."}

  {:el :class
   :id :overarch.data-model/context-boundary
   :name "context-boundary"
   :desc "A boundary of a bounded context."
   :tags #{"architecture-model" "concept-model"}}

  {:el :class
   :id :overarch.data-model/enterprise-boundary
   :name "enterprise-boundary"
   :desc "A boundary of an enterprise or a company."
   :tags #{"architecture-model" "concept-model"}}

  {:el :inheritance
   :id :overarch.data-model/context-boundary-is-a-boundary-node
   :to :overarch.data-model/boundary-node
   :from :overarch.data-model/context-boundary
   :tags #{"architecture-model" "concept-model" "element-hierarchy"}}

  {:el :inheritance
   :id :overarch.data-model/enterprise-boundary-is-a-boundary-node
   :to :overarch.data-model/boundary-node
   :from :overarch.data-model/enterprise-boundary
   :tags #{"architecture-model" "concept-model" "element-hierarchy"}}

  ;;
  ;; Architecture Model
  ;;
  {:el :class
   :id :overarch.data-model/architecture-model-element
   :abstract true
   :name "architecture-model-element"
   :desc "An element of the architecture model."
   :tags #{"architecture-model"}
   :ct [{:el :field
         :name "external"
         :type "boolean"
         :optional true
         :desc "When true, the element plays a role in the model but it is not in the scope of the project. Defaults to false."}]}

  {:el :class
   :id :overarch.data-model/architecture-model-node
   :abstract true
   :name "architecture-model-node"
   :desc "A node in the architecture model."
   :tags #{"architecture-model"}}

  {:el :class
   :id :overarch.data-model/technical-architecture-model-element
   :abstract true
   :name "technical-architecture-model-element"
   :desc "A technical element of the architecture, e.g. a system or container."
   :tags #{"architecture-model"}}

  {:el :class
   :id :overarch.data-model/technical-architecture-model-node
   :abstract true
   :name "technical-architecture-model-node"
   :desc "A technical node in the architecture model."
   :tags #{"architecture-model"}
   :ct [{:el :field
         :name "subtype"
         :type "string"
         :optional true
         :desc "Subtype of the element, one of :database or :queue."}]}

  {:el :class
   :id :overarch.data-model/architecture-model-relation
   :abstract true
   :name "architecture-model-relation"
   :desc "A relation in the architecture model."
   :tags #{"architecture-model"}}

  {:el :class
   :id :overarch.data-model/technical-architecture-model-relation
   :abstract true
   :name "technical-architecture-model-relation"
   :desc "A technical relation in the architecture model."
   :tags #{"architecture-model"}}

  ;; Person
  {:el :class
   :id :overarch.data-model/person
   :name "person"
   :desc "A human actor or role working with the system under description. A person can be used in the architecture model and the use case model."
   :tags #{"architecture-model" "use-case-model"}}

  {:el :class
   :id :overarch.data-model/system
   :name "system"
   :desc "A system relevant in the architecture.
          A system can be an external system, which is modelled as a black box
          or an internal system, a system under description, which is modelled as a compound element with all the containers of the system.
          A system can be used in the architecture model, the deployment model (external systems) and the use case model (external systems)."
   :tags #{"architecture-model" "deployment-model" "use-case-model"}}

  {:el :class
   :id :overarch.data-model/container
   :name "container"
   :desc "A container is a part of a system and describes a deployed process in the architecture (e.g. a service or an application).
          A container is a compound element which contains the components of the implementation.
          A container can be used in the architecture model, the deployment model and the use case model."
   :tags #{"architecture-model" "deployment-model" "use-case-model"}}

  {:el :class
   :id :overarch.data-model/component
   :name "component"
   :desc "A compontent is a part of a container and describes a (logical) building block of a container (e.g. a module or a layer)."
   :tags #{"architecture-model"}}

  {:el :class
   :id :overarch.data-model/request
   :name "request"
   :desc "A synchronous request between two elements of the architecture."
   :tags #{"architecture-model"}}

  {:el :class
   :id :overarch.data-model/response
   :name "response"
   :desc "A response to a synchronous request between two elements of the architecture."
   :tags #{"architecture-model"}}

  {:el :class
   :id :overarch.data-model/dataflow
   :name "dataflow"
   :desc "A flow of data between two elements of the architecture."
   :tags #{"architecture-model"}}

  {:el :class
   :id :overarch.data-model/send
   :name "send"
   :desc "An asynchronous message or command between two elements of the architecture (point-to-point)."
   :tags #{"architecture-model"}}

  {:el :class
   :id :overarch.data-model/publish
   :name "publish"
   :desc "Publishing of asynchronous events between two elements of the architecture (receiver should be a broker or topic)."
   :tags #{"architecture-model"}}

  {:el :class
   :id :overarch.data-model/subscribe
   :name "subscribe"
   :desc "Subscription of asynchronous events between two elements of the architecture (sender should be a broker or topic)."
   :tags #{"architecture-model"}}

  ;; Architecture Model Relations
  {:el :inheritance
   :id :overarch.data-model/architecture-model-element-is-a-identifiable-named-element
