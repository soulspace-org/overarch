  ;;
  ;; Class Model
  ;;
  {:el :class
   :id :overarch.data-model/typed-node
   :abscract true
   :name "typed-node"
   :desc "A node which may have a type field."
   :tags #{"class-model" "type-hierarchy"}
   :ct #{{:el :field
          :name "type"
          :type "string"
          :optional true
          :desc "The type of the element."}}}

  {:el :class
   :id :overarch.data-model/optional-node
   :abscract true
   :name "optional-node"
   :desc "A node which may have an optional field."
   :tags #{"class-model" "type-hierarchy"}
