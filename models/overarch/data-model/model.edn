  ;;
  ;; Deployment Model
  ;;
  {:el :class
   :id :overarch.data-model/deployment-model-node
   :abstract true
   :name "deployment-model-node"
   :desc "A node in the deployment model."}

  {:el :class
   :id :overarch.data-model/deployment-model-relation
   :abstract true
   :name "deployment-model-relation"
   :desc "A relation in the deployment model."}

  {:el :class
   :id :overarch.data-model/deployment-model-element
   :abstract true
   :name "deployment-model-element"
   :desc "An element in the deployment model."}

  {:el :class
   :id :overarch.data-model/node
   :name "node"
   :desc "An element of the deployment model of the system under description.
          A node is a compound element which contains other nodes or containers
          referenced from the architecture model."}

  {:el :class
   :id :overarch.data-model/link
   :name "link"
   :desc "A link between two nodes of the deployment model."}

  {:el :class
   :id :overarch.data-model/deployed-to
   :name "deployed-to"
   :desc "A deployment relation between a container and a node in the deployment model. The container is deployed to the node."}

